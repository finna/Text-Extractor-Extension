{
    "manifest_version": 3,
    "name": "Text Extractor",
    "description": "Extracts text from the current webpage and displays it.",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_title": "Extract Text"
    }
  }
  