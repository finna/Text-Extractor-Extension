{
    "manifest_version": 3,
    "name": "Tweet Extractor",
    "version": "1.0",
    "description": "Extract and save tweets",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    }
  }
